{
  "name": "@woocommerce/woocommerce-rest-api",
  "version": "1.0.1",
  "description": "WooCommerce REST API - JavaScript Library",
  "author": "Automattic",
  "license": "MIT",
  "keywords": [
    "wordpress",
    "woocommerce",
    "rest",
    "promise",
    "node"
  ],
  "homepage": "https://github.com/woocommerce/woocommerce-rest-api-js-lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woocommerce/woocommerce-rest-api-js-lib.git"
  },
  "bugs": {
    "url": "https://github.com/woocommerce/woocommerce-rest-api-js-lib/issues"
  },
  "main": "index",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts"
  ],
  "dependencies": {
    "axios": "^1.5.0",
    "oauth-1.0a": "^2.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "7.22.11",
    "@babel/plugin-transform-modules-commonjs": "7.22.11",
    "@babel/preset-env": "7.22.14",
    "babel-jest": "29.6.4",
    "del-cli": "^5.0.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-standard": "4.1.0",
    "husky": "8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "14.0.1",
    "nock": "13.3.3",
    "prettier": "3.0.3"
  },
  "scripts": {
    "build": "del index.js && babel index.mjs --out-dir .",
    "test": "npm run build && jest",
    "format": "prettier --write \"*.mjs\" \"test.js\"",
    "lint": "eslint *.mjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,mjs}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=14"
  }
}